<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fffccc"
    tools:context="com.example.come.come.HomecomingActivity">

    <SeekBar
        android:id="@+id/home_seekBar"
        style="@style/Widget.AppCompat.SeekBar.Discrete"
        android:layout_width="346dp"
        android:layout_height="43dp"
        android:max="12"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="295dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintHorizontal_bias="0.508"
        tools:layout_editor_absoluteY="287dp"
        tools:layout_editor_absoluteX="10dp" />

    <ImageButton
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:id="@+id/backButton"
        android:layout_gravity="center_horizontal"
        android:padding="0sp"
        android:scaleType="centerCrop"
        android:background="#00000000"
        android:src="@drawable/back"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="27dp"
        android:layout_marginTop="47dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="47dp" />

    <ImageButton
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/sendButton"
        android:layout_gravity="center_horizontal"
        android:padding="0sp"
        android:scaleType="centerCrop"
        android:background="#00000000"
        android:src="@drawable/house"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="170dp"
        android:layout_marginTop="490dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="199dp" />

    <TextView
        android:id="@+id/textView_home_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textSize="30sp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/home_seekBar" />

</android.support.constraint.ConstraintLayout>
